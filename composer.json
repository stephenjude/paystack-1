{
    "name": "xeviant/paystack",
    "description": "A Paystack API Wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olatunbosun Egberinde",
            "email": "bosunski@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3|^7.2",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "php-http/httplug": "1.0.0|2.0.0",
        "php-http/discovery": "^1.6",
        "php-http/client-common": "^2.3",
        "php-http/cache-plugin": "^1.7",
        "illuminate/support": "^5.7|^5.8|^6.0|^7.0|^8.0",
        "league/event": "^2.2",
        "illuminate/container": "^5.7|^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "guzzlehttp/psr7": "^1.5",
        "cache/array-adapter": "^1.0",
        "php-http/guzzle6-adapter": "^2.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/mock-client": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Xeviant\\Paystack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xeviant\\Paystack\\Tests\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
